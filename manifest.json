{
  "manifest_version": 2,
  "name": "SkipTo",
  "version": "0.3",

  "description" : "",
  "homepage_url": "https://github.com/skipto/extension",
  "author": "Nicholas Hoyt",

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_popup": "popup.html"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+0"
      }
    }
  }
}
